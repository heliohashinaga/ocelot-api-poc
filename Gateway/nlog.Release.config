<?xml version="1.0" encoding="utf-8"?>
<!--For more information on using transformations see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" autoReload="true" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <targets>
    <target name="bufferedDatabase" bufferSize="50" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
  </targets>
  <rules>
    <logger name="*" minlevel="Warn" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
  </rules>
</nlog>